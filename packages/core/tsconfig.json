{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true,
        "outDir": "./dist",
        "declaration": true,
        "declarationDir": "./dist",
        "lib": ["ESNext", "DOM"],
        "downlevelIteration": true
    }
}
